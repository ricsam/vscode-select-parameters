{
	"name": "selectparameters",
	"displayName": "Select Parameters",
	"description": "Select every parameter in functions or react components. It will also select every key in objects or every item in arrays.",
	"version": "0.1.1",
	"publisher": "richie",
	"engines": {
		"vscode": "^1.36.0"
	},
	"license": "MIT",
	"categories": [
		"Other"
	],
	"keywords": [
		"jsx",
		"react",
		"select",
		"typescript",
		"javascript"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/ricsam/vscode-select-parameters"
	},
	"activationEvents": [
		"onCommand:extension.selectParameters"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.selectParameters",
				"title": "Select Parameters"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.0-next.0"
	}
}
